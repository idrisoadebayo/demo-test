pipeline{
    agent any

    stages {
    stages('Git checkout') {
                   steps {
                        checkout changelog: false, poll: false, scm: scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[credentialsId: 'for-github', url: 'https://github.com/idrisoadebayo/demo-test.git']])
                   }
               }
    stage('value')
    {
        steps{
            sh "mvn validate"
        }
    }
    }
}