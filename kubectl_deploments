apiVersion: apps/v1
kind: Deployment
metadata:
  name: mydeployment
  namespace: ilyas
  labels:
    app: idriswebapp
spec:
  replicas: 3
  selector:
    matchLabels:
      app: idriswebapp
  template:
    metadata:
      labels:
        app: idriswebapp
    spec:
      containers:
      - name: mynginxcontainer
        image: nginx:latest
        ports:
        - containerPort: 80